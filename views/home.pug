extends layouts/main
include mixins/videoBlock

block content
    .home-videos
        // videos가 되는 객체를 videoController에서 console.log 해보면 확인 가능
        each item in videos
            +videoBlock({
                id: item.id,
                title: item.title,
                views: item.views,
                videoFile: item.fileUrl 
            })